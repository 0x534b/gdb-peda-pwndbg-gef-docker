define init-peda
source ~/gdb-peda-pwndbg-gef/peda/peda.py
end
document init-peda
Initializes the PEDA (Python Exploit Development Assistant for GDB) framework
end

define init-pwndbg
source ~/gdb-peda-pwndbg-gef/pwndbg/gdbinit.py
end
document init-pwndbg
Initializes PwnDBG
end

define init-gef
source ~/gdb-peda-pwndbg-gef/gef/gef.py
end
document init-gef
Initializes GEF (GDB Enhanced Features)
end
